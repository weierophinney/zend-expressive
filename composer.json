{
    "name": "zendframework/zend-expressive",
    "description": "PSR-7 Middleware Microframework based on Stratigility",
    "type": "library",
    "license": "BSD-2-Clause",
    "keywords": [
        "http",
        "middleware",
        "psr",
        "psr-7"
    ],
    "repositories": [
      {
        "type": "vcs",
        "url": "https://github.com/ezimuel/zend-stratigility-dispatch.git"
      }
    ],
    "require": {
        "zendframework/zend-stratigility": "^1.1",
        "psr/http-message": "^1.0",
        "container-interop/container-interop": "^1.1"
    },
    "require-dev": {
        "zendframework/zend-diactoros": "^1.1",
        "phpunit/phpunit": "^4.7",
        "squizlabs/php_codesniffer": "^2.3",
        "nikic/fast-route": "^0.6.0"
    },
    "autoload": {
      "psr-4": {
        "Zend\\Expressive\\": "src/"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "ZendTest\\Expressive\\": "test/"
      }
    },
    "suggest": {
        "nikic/fast-route": "^0.6.0 to use the FastRoute routing adapter"
    }
}
